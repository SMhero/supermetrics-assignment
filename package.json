  "description": "Simple post reader app based on ReactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.ts",
    "deploy": "gh-pages -d dist",
    "lint:css": "`npm bin`/stylelint \"src/**/*.css\" --formatter verbose",
    "lint:js": "`npm bin`/eslint src --ext .ts --ext .tsx --quiet",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "webpack serve --config webpack.config.ts --host 0.0.0.0 --progress",
    "test": "npm run test"
  },
